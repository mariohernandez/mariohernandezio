{
  "name": "mariohernandezio",
  "version": "1.0.0",
  "description": "A personal blog website built with Eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npx eleventy --serve",
    "production": "NODE_ENV=production postcss src/css/styles.css --dir dist/css && NODE_ENV=production npx eleventy",
    "watch:css": "postcss src/css/styles.css --dir dist/css -w",
    "compile:css": "postcss src/css/styles.css --dir dist/css"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "last 2 version",
    "safari 16"
  ],
  "keywords": [],
  "author": "Mario Hernandez",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^2.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cssnano": "^7.0.2",
    "eleventy-plugin-reading-time": "^0.0.1",
    "esbuild": "^0.21.5",
    "html-minifier": "^4.0.0",
    "markdown-it": "^14.1.0",
    "markdown-it-attrs": "^4.1.6",
    "moment": "^2.29.4",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.5.14",
    "stylelint": "^16.6.1",
    "terser": "^5.31.1"
  }
}
