{
  "name": "mariohernandezio",
  "version": "1.0.0",
  "description": "A personal blog website built with Eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npx eleventy --serve",
    "production": "NODE_ENV=production postcss src/css/styles.css --dir dist/css && NODE_ENV=production npx eleventy",
    "watch:css": "postcss src/css/styles.css --dir dist/css -w",
    "compile:css": "postcss src/css/styles.css --dir dist/css"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "last 2 version",
    "safari 16"
  ],
  "keywords": [],
  "author": "Mario Hernandez",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-rss": "^2.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cssnano": "^7.0.6",
    "esbuild": "^0.24.2",
    "html-minifier": "^4.0.0",
    "markdown-it": "^14.1.0",
    "markdown-it-attrs": "^4.3.1",
    "moment": "^2.29.4",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.1.3",
    "stylelint": "^16.14.1",
    "terser": "^5.37.0"
  }
}
