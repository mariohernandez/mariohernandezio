{
  "name": "mariohernandezio",
  "version": "1.0.0",
  "description": "A personal blog website built with Eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npx gulp && concurrently \"npx gulp watch\" \"npx eleventy --serve\"",
    "production": "NODE_ENV=production npx gulp && postcss src/css/styles.css --dir dist/css && NODE_ENV=production npx eleventy",
    "watch:css": "postcss src/css/styles.css --dir dist/css -w",
    "compile:css": "postcss src/css/styles.css --dir dist/css"
  },
  "keywords": [],
  "author": "Mario Hernandez",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "html-minifier": "^4.0.0",
    "markdown-it": "^14.0.0",
    "markdown-it-attrs": "^4.1.6",
    "moment": "^2.29.4",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "stylelint": "^16.0.1"
  }
}
